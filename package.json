{
  "name": "aracari",
  "version": "0.0.1",
  "description": "A simple, conversational budgeting app",
  "repository": {
    "type": "git",
    "url": "https://github.com/joshwcomeau/aracari.git"
  },
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=development nodemon server/index.js",
    "start:production": "npm run build && NODE_ENV=production nodemon server/index.js",
    "webpack:json": "webpack --config webpack.prod.js --json > stats.json",
    "test": "NODE_ENV=test NODE_PATH=$NODE_PATH:$PWD/client mocha --compilers js:babel-core/register --require ./test/test_helper.js --require ignore-styles --recursive",
    "lint": "eslint -c .eslintrc client/** --ext .js --ext .jsx",
    "deploy": "NODE_ENV=production fly production",
    "storybook": "start-storybook -p 9001",
    "build": "webpack --config webpack.prod.js -p"
  },
  "author": "Joshua Comeau",
  "license": "MIT",
  "dependencies": {
    "babel": "^6.3.13",
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.17",
    "babel-loader": "^6.2.0",
    "babel-plugin-react-transform": "^2.0.0-beta1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.3.13",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.1.0",
    "babel-preset-stage-0": "^6.3.13",
    "babel-runtime": "^6.3.13",
    "body-parser": "^1.15.1",
    "classnames": "^2.2.1",
    "express": "^4.13.4",
    "flightplan": "^0.6.14",
    "history": "~1.17.0",
    "isomorphic-fetch": "^2.2.0",
    "lodash": "^4.12.0",
    "material-ui": "^0.15.0",
    "moment": "^2.11.2",
    "nconf": "^0.8.4",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-flip-move": "^2.3.0",
    "react-motion": "^0.4.4",
    "react-redux": "^4.4.0",
    "react-router": "^2.4.1",
    "react-router-redux": "^4.0.4",
    "react-swipe-to-reveal-options": "joshwcomeau/react-swipe-to-reveal-options",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "^3.5.2",
    "redux-form": "^5.2.5",
    "redux-localstorage": "^0.4.1",
    "redux-saga": "^0.10.5",
    "redux-thunk": "^1.0.0",
    "reselect": "^2.5.1",
    "slug": "^0.9.1",
    "uuid": "^2.0.2"
  },
  "devDependencies": {
    "@kadira/storybook": "^1.26.0",
    "autoprefixer": "^6.2.3",
    "chai": "^3.4.1",
    "chai-equal-jsx": "^1.0.9",
    "css-loader": "^0.23.0",
    "enzyme": "^2.3.0",
    "eslint": "^2.10.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.8.0",
    "eslint-plugin-jsx-a11y": "^1.2.0",
    "eslint-plugin-react": "^5.1.1",
    "ignore-styles": "^2.0.0",
    "jsdom": "^9.1.0",
    "mocha": "^2.3.4",
    "node-sass": "^3.4.2",
    "postcss-loader": "^0.8.0",
    "react-addons-perf": "^15.1.0",
    "react-addons-test-utils": "^15.1.0",
    "react-transform-hmr": "^1.0.0",
    "redux-devtools": "^3.0.0",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.1",
    "sass-loader": "^3.1.2",
    "sinon": "^1.17.2",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.0",
    "supertest": "^1.1.0",
    "webpack": "^1.12.9",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.2.0"
  },
  "babel": {
    "plugins": [
      [
        "transform-decorators-legacy"
      ]
    ],
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ]
  }
}
